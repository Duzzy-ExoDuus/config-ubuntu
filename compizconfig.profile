[commands]
s0_run_command0_key = <Super>Left
s0_run_command1_key = <Super>Right
s0_run_command2_key = <Super>Up
s0_run_command0_edge = Left
s0_run_command1_edge = Right
s0_run_command2_edge = Top

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9
s0_edgeflip_move = true

[grid]
s0_put_center_key = <Super>space
s0_put_left_key = <Super>Left
s0_put_right_key = <Super>Right
s0_put_top_key = <Super>Up
s0_put_bottom_key = Disabled
s0_put_restore_key = <Super>Down

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[animation]
s0_unminimize_effects = animation:Glide 2;

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Control><Alt>t

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[expo]
s0_distance = 0.005000
s0_vp_saturation = 40.000000
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[core]
s0_active_plugins = core;composite;opengl;copytex;vpswitch;compiztoolbox;move;snap;mousepoll;place;grid;resize;wall;imgpng;regex;unitymtgrabhandles;animation;workarounds;session;expo;fade;ezoom;scale;unityshell;
s0_edge_delay = 400
s0_outputs = 1920x1080+0+0;1920x1080+1920+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_hsize = 2
s0_vsize = 2

[unityshell]
s0_show_desktop_key = <Super>d
s0_inactive_shadow_color = #000000a5
s0_num_launchers = 1
s0_launcher_capture_mouse = false
